<script type="text/javascript" src="http://cdnjs.cloudflare.com/ajax/libs/jspdf/1.5.3/jspdf.min.js"></script>				
<script>

var bill = $("#pdfArea").val()

if (bill == null) {
	Swal.fire({
		   title :  '', 
		   html:  '<strong>조회하실 데이터가 존재하지 않습니다.</strong>', 
		   icon : 'error'
		} );
	//alert("조회하실 데이터가 존재하지 않습니다.");
	$("#semesterType").prop('disabled', true);
}

$("select[name=semesterType]").change(function () {
	//var sel = $(this).val(); //value값 가져오기
	var semester = $(this).val();

	console.log(semester);

	if (semester == "one") {
		$("#pdfArea").css("display", "block");
		$("#download").css("display", "block");
	}

	if (semester == "two") {
		$("#pdfArea").css("display", "none");
		$("#download").css("display", "none");
		Swal.fire({
			   title :  '',
			   html:  '<strong>해당학기 조회기간이 아닙니다!</strong>', 
			   icon : 'warning'
		});
		//alert("해당학기 조회 기간이 아닙니다.");
	}

});

function saveImage() {
	const element = document.getElementById("pdfArea");
	const options = {
		allowsTaint : true, //cross-origin allow
		useCORS : true, //CORS 사용한 서버로부터 이미지 로드할 것인지 여부
		scale : 2, //기본 해상도 2배
		//이 부분 이용해서 화면 캡쳐할 크기 지정
		width : 1200,
		height : 800
	};

	html2canvas(element, options).then(
			function(canvas) {

				const pdf = new jsPDF("p", "mm", "a4"); //'p'는 포트레이트, 'mm'은 밀리미터 단위, 'a4'는 용지 크기를 의미
				//만약 다른 용지 크기를 사용하려면, 위 코드에서 'a4'를 원하는 용지 크기로 변경하면 됨

				let imgWidth = 210; //이미지 가로길이(mm)
				let pageHeight = 295; // 출력페이지 세로길이 
				let imgHeight = canvas.height * imgWidth / canvas.width;
				let heightLeft = imgHeight;

				// 이미지의 x,y축 위치 옮기기
				let xposition = 20;
				let yposition = 10;
				//addImage(imageData, format, x, y, width, height, alias, compression, rotation) 
				pdf.addImage(canvas, 'JPEG', xposition, yposition,
						imgWidth, imgHeight);
				heightLeft -= pageHeight;

				//이미지가 페이지보다 길어질 경우 잘리지 않도록 while문 사용 
				while (heightLeft >= 0) {
					position = heightLeft - imgHeight;
					pdf.addPage();
					pdf.addImage(canvas, 'JPEG', xposition, yposition,
							imgWidth, imgHeight);
					heightLeft -= pageHeight;
				}
				//아래 이름으로PDF 파일을 다운로드한다.
				pdf.save("등록금고지서.pdf");
			});
}
</script>
