/*--------------------------------------------------------------
# General
--------------------------------------------------------------*/
body {
  font-family: "Open Sans", sans-serif;
  color: #444444;
}

a {
  color: #22313f;
  text-decoration: none;
}

a:hover {
  color: #51776c;
  text-decoration: none;
}

h1,
h2,
h3,
h4,
h5,
h6 {
  font-family: "Roboto", sans-serif;
}


/*--------------------------------------------------------------
# Back to top button
--------------------------------------------------------------*/
.back-to-top {
  position: fixed;
  visibility: hidden;
  opacity: 0;
  right: 15px;
  bottom: 15px;
  z-index: 996;
  background: #22313f;
  width: 40px;
  height: 40px;
  border-radius: 4px;
  transition: all 0.4s;
}

.back-to-top i {
  font-size: 28px;
  color: #fff;
  line-height: 0;
}

.back-to-top:hover {
  background: #22313f;
  color: #fff;
}

.back-to-top.active {
  visibility: visible;
  opacity: 1;
}

/*--------------------------------------------------------------
# Disable aos animation delay on mobile devices
--------------------------------------------------------------*/
@media screen and (max-width: 768px) {
  [data-aos-delay] {
    transition-delay: 0 !important;
  }
}

/*--------------------------------------------------------------
# Top Bar
--------------------------------------------------------------*/
#topbar {
  background: #22313f;
  height: 35px;
  font-size: 13px;
  transition: all 0.5s;
  color: #fff;
  padding: 0;
}

#topbar .contact-info i {
  font-style: normal;
  color: #fff;
}

#topbar .contact-info i a,
#topbar .contact-info i span {
  padding-left: 5px;
  color: #fff;
}

#topbar .contact-info i a {
  padding-left: 10px;
  line-height: 0;
  transition: 0.3s;
  transition: 0.3s;
  letter-spacing: 1px;
}

#topbar .contact-info i a:hover {
  color: #fff;
}

#topbar .login-links a i{
  /* color: rgba(255, 255, 255, 0.7); */
  color: #fff;
  line-height: 0;
  transition: 0.3s;
  margin-left: 20px;
  font-style: normal;
}

#topbar .login-links a i span{
  /* color: rgba(255, 255, 255, 0.7); */
  margin-left: 5px;

}

#topbar .login-links a:hover {
  color: #fff;
}

